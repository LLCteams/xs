VexHubLogo.Parent = ImageFrame
VexHubLogo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
VexHubLogo.BackgroundTransparency = 1.000
VexHubLogo.BorderColor3 = Color3.fromRGB(0, 0, 0)
