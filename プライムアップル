	local btn = script.Parent
	local Gui = script.Parent.Parent.Parent
	local Krnl = script.Parent.Parent.Parent.Parent.FloatingGUI
	
	btn.MouseButton1Click:Connect(function()
		Gui.Visible = false
		Krnl.Visible = true
	end)
end
