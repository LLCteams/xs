SubtitleLabel.BackgroundTransparency = 1
SubtitleLabel.Position = UDim2.new(0, 0, 0, 35)
SubtitleLabel.Size = UDim2.new(1, 0, 0, 20)

KeyTextBox.Name = "KeyTextBox"
KeyTextBox.Parent = MainFrame
KeyTextBox.PlaceholderText = "Insert Key..."
KeyTextBox.Font = Enum.Font.SourceSans
KeyTextBox.TextSize = 14
KeyTextBox.TextColor3 = Color3.fromRGB(178, 178, 178)
KeyTextBox.BackgroundColor3 = Color3.fromRGB(22, 22, 22)
KeyTextBox.Size = UDim2.new(0.9, 0, 0, 30)
KeyTextBox.Position = UDim2.new(0.05, 0, 0, 60)
KeyTextBox.BorderSizePixel = 0

TextBoxFrame.Name = "TextBoxFrame"
TextBoxFrame.Parent = KeyTextBox
TextBoxFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TextBoxFrame.Size = KeyTextBox.Size
TextBoxFrame.Position = KeyTextBox.Position

TextBoxCorner.CornerRadius = UDim.new(0, 4)
TextBoxCorner.Parent = KeyTextBox

CloseButton.Parent = MainFrame
CloseButton.Text = "X"
CloseButton.Font = Enum.Font.SourceSansBold
CloseButton.TextSize = 18
CloseButton.TextColor3 = Color3.fromRGB(255, 255, 255)
CloseButton.BackgroundTransparency = 1
CloseButton.Position = UDim2.new(0, 5, 0, 5)
CloseButton.Size = UDim2.new(0, 20, 0, 20)

CloseButton.MouseButton1Click:Connect(function()
    MainFrame:TweenPosition(UDim2.new(-1.5, 0, 0.5, 0), Enum.EasingDirection.InOut, Enum.EasingStyle.Quad, 0.5, true, function()
        ScreenGui:Destroy()
    end)
end)

