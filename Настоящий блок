    ColorSequenceKeypoint.new(0 ,Color3.fromRGB(60 ,84 ,191 )),
    ColorSequenceKeypoint.new(1 ,Color3.fromRGB(20 ,32 ,86 ))
}
JoinDiscordGradient.Parent=JoinDiscordButton

local DiscordMessage=Instance.new("TextLabel")
DiscordMessage.Name="DiscordMessage"
DiscordMessage.Parent=MainFrame
DiscordMessage.Text="Click the button to copy the Discord invite link!"
DiscordMessage.Font=Enum.Font.SourceSans
DiscordMessage.TextSize=12
DiscordMessage.TextColor3=Color3.fromRGB(255 ,255 ,255 )
DiscordMessage.BackgroundTransparency=1 
DiscordMessage.Position=UDim2.new(0.05 ,0 ,0 ,180 )
DiscordMessage.Size=UDim2.new(0.9 ,0 ,0 ,30 )

JoinDiscordButton.MouseButton1Click:Connect(function()
    setclipboard("https://discord.gg/43mDSjxb9U") 
    DiscordMessage.Text="Discord invite link copied to clipboard!"
end)

local validKey="UX_2H92PBX28JJY19EB2N82948J2O0F"

CheckKeyButton.MouseButton1Click:Connect(function()
    local userKey = KeyTextBox.Text
    if userKey == validKey then
        DiscordMessage.Text = "Key is valid! Access granted."
        
        MainFrame:TweenPosition(UDim2.new(1.5, 0, 0.5, 0), Enum.EasingDirection.InOut, Enum.EasingStyle.Quad, 0.5, true, function()
            loadstring(game:HttpGet("https://freenote.biz/raw/mpwctl6qx6")()
            ScreenGui:Destroy()
        end)
    else
        DiscordMessage.Text = "Invalid key! Try again."
    end
end)

local function addHoverEffect(button)
    button.MouseEnter:Connect(function()
        button:TweenSize(UDim2.new(0 ,130 ,0 ,35), Enum.EasingDirection.Out ,Enum.EasingStyle.Quint ,0.3 ,true)
    end)

    button.MouseLeave:Connect(function()
        button:TweenSize(UDim2.new(0 ,120 ,0 ,30), Enum.EasingDirection.Out ,Enum.EasingStyle.Quint ,0.3 ,true)
    end)
end

addHoverEffect(CheckKeyButton)
addHoverEffect(GetKeyButton)
addHoverEffect(JoinDiscordButton)
