    ColorSequenceKeypoint.new(0, Color3.fromRGB(170, 0, 0)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(100, 0, 0))
}
GetKeyGradient.Parent = GetKeyButton

GetKeyButton.MouseButton1Click:Connect(function()
    setclipboard("https://link-target.net/780766/key-system-1")
    DiscordMessage.Text = "Key link copied to clipboard!"
end)

JoinDiscordButton.Name = "JoinDiscordButton"
