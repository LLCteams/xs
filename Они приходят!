local DiscordLib =
    loadstring(game:HttpGet "https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/discord")()

local win = DiscordLib:Window("Loader X")

local serv = win:Server("Loader", "1")

local btns = serv:Channel("Universe X")

btns:Button(
    "Hide And Seek",
    function()
        DiscordLib:Notification("Notify", "Executed!", "Okay!")
        BETA()
    end
)

btns:Seperator()

