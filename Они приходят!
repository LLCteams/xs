end)
local vocal = Luxt:Tab("Character")
local kol = vocal:Section("Movement")
kol:Button("Kill All Hiders", function()
local localPlayer = game.Players.LocalPlayer
local localPlayerCF = localPlayer.Character.HumanoidRootPart.CFrame

for _, player in pairs(game.Players:GetPlayers()) do
    if player ~= localPlayer then
        player.Character.HumanoidRootPart.CFrame = localPlayerCF
        player.Character.Humanoid.PlatformStand = true
        player.Character.Humanoid.WalkSpeed = 0
        player.Character.Humanoid.JumpPower = 0
        for _, part in pairs(player.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Anchored = true
            end
        end
    end
end

localPlayer.Character.HumanoidRootPart.Anchored = false
end)
kol:Toggle("Anti-kick", function(pop)
     if pop == false then
           print('Universe clients has Disabled.')
        else
           print('Universe clients has Enabled.')
       end
end)
kol:Label('Character')
kol:TextBox("WalkSpeed", "Number/16", function(pp)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = pp
end)
kol:TextBox("JumpHeight", "Number/50", function(pop)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = pop
end)
local bol = vocal:Section('Other')
local IsNoClipEnabled = false
bol:Toggle('Noclip', function(enabled)
isNoClipEnabled = enabled
    local character = game.Players.LocalPlayer.Character

    while isNoClipEnabled do
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
        game:GetService("RunService").Stepped:Wait()
    end
end)
local InfiniteJumpEnabled;
bol:Toggle('Infinite Jump',function(State)

InfiniteJumpEnabled = State
end)
bol:TextBox("Fov", "Amount/80", function(txt)
game.Workspace.CurrentCamera.FieldOfView = txt
end)
bol:TextBox("Gravity", "Amount/??", function(boy)
workspace.Gravity = boy
end)
end
