    "{•} Console",
    function()
        game:GetService("StarterGui"):SetCore("DevConsoleVisible",true)
    end
)

ovoi:Button(
    "{•} Respawn",
    function()
        DiscordLib:Notification("Notify", "Loading..", "Okay!")
        LocalPlayer.Character:WaitForChild("Humanoid").Health = 0
    wait()
    end
)

ovoi:Button(
    "{•} Enable Reset",
    function()
        DiscordLib:Notification("Notify", "Loading..", "Okay!")
        game:GetService("StarterGui"):SetCore("ResetButtonCallback", enablereset)
    end
)

ovoi:Button(
    "{•} Reset",
    function()
        DiscordLib:Notification("Notify", "Loading..", "Okay!")
        game.Players.LocalPlayer.Character.Head:Destroy()
    end
)

ovoi:Button(
    "{•} Rejoin",
    function()
        DiscordLib:Notification("Notify", "Script in-game has executed!", "Okay!")
        game:GetService("TeleportService"):Teleport(game.PlaceId, Player)
    end
)

ovoi:Button(
    "{•} Leave",
    function()
        DiscordLib:Notification("Notify", "Script in-game has executed!", "Okay!")
        game:Shutdown()
    end
)

