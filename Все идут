wait(1)

game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 22

local Luxtl = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Luxware-UI-Library/main/Source.lua"))()
local Luxt = Luxtl.CreateWindow("Volt Limited", 13951232354)

--- Placez
local GameId = game.PlaceId
local GameName = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId).Name
local plrs = game:GetService("Players")
local plr = plrs.LocalPlayer
local plrname = plr.Name
local plrid = plr.UserId
local plrchr = plr.Character
local plrage = plr.AccountAge
local plrcount = #game.Players:GetPlayers()

local mainTab = Luxt:Tab("Home")
local pu2 = mainTab:Section("Self")
pu2:Label("Username: "..game.Players.LocalPlayer.Name)
pu2:Button("UID: "..game.Players.LocalPlayer.UserId)
local pu3 = mainTab:Section("Game")
pu3:Button("Gamename: "..GameName, function()
      print('?')
end)
pu3:Button("GID: "..game.PlaceId.."")
pu3:Button("Serverplayer: " ..plrcount.. " MaxPlayers Joined")

local speed = Luxt:Tab("Character")
local sad = speed:Section("Movement")
sad:TextBox("WalkSpeed [16 Default]", "Amount", function(gt)

NotificationLibrary:SendNotification("Success", "Speed has changed !", 4)

game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = gt
end)
sad:TextBox("JumpPower [50 Default]", "Amount", function(ht)

NotificationLibrary:SendNotification("Success", "Jump has changed !", 4)

game.Players.LocalPlayer.Character.Humanoid.JumpPower = ht
end)
sad:Label("More Speed")
sad:Slider("Walk Speed", 16, 503, function(currentValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = currentValue 
end)
sad:Slider("Jump Power", 50, 560, function(lolvalue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = lolvalue
end)
local sads = speed:Section("Other")
sads:TextBox("Chat [Message]", "Type your chat", function(var)
local var = string.sub(msg,space+1)
		                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(var, "All")
		
end)
sads:TextBox("Fov [80 Default]", "Amount", function(txt)

NotificationLibrary:SendNotification("Success", "Fov has changed !", 4)

game.Workspace.CurrentCamera.FieldOfView = txt
end)
sads:TextBox("Gravity [196.2 Default]", "Amount", function(boy)

NotificationLibrary:SendNotification("Success", "Gravity has changed !", 4)

workspace.Gravity = boy
end)

local visual = Luxt:Tab("Visual")
local visa = visual:Section("Esp 2")
