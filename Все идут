wait(0.1)
game.Players.LocalPlayer.Character.Animate.Disabled = false

wait(2)

ff = Instance.new("ForceField", game.Players.LocalPlayer.Character)
ff.Visible = true
 
-- Sets health to max
maxHealth = game.Players.LocalPlayer.Humanoid.MaxHealth
player.Health = maxHealth
end)
