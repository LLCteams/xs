function AZE()

--Made by : https://v3rmillion.net/member.php?action=profile&uid=1265584

_G.Settings = {
['Name'] = 'Volt',
['Intro'] = false,
['Keybind'] = 'F'
}

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/LLCteams/ZKHub/main/%D0%9D%D0%BE%D0%B2%D1%8B%D0%B9%2C%20%D0%BD%D0%BE%20%D1%81%D1%82%D0%B0%D1%80%D1%8B%D0%B9"))()

local Tab1 = Library:CreateTab('1')

Tab1:Label('Oldest version')
Tab1:Button('Volt hub', function()
game.CoreGui.ShadowLib:Destroy()
bonuses()
end)
Tab1:Button('Volt hub', function()
game.CoreGui.ShadowLib:Destroy()
haira()
end)
Tab1:Label('Status: Working')

local Tab2 = Library:CreateTab('2')

Tab2:Label('New version')
Tab2:Label('v5.0.0 in fixing !')

local Tab3 = Library:CreateTab('3')

Tab3:Label('Game Script')
Tab3:Button('Breaking Point', function()
dupebp()
end)
Tab3:Button('Zombie Uprising', function()
loadstring(game:HttpGet("https://pastebin.com/raw/0N1y7RyM"))() 
end)
Tab3:Button('Prison Life', function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/LLCteams/ZKHub/main/%D0%9A%D1%82%D0%BE%20%D1%83%D0%BA%D1%80%D0%B0%D0%BB%20%D1%87%D0%B5%D1%80%D1%82%D0%B5%D0%B6'))()
end)
Tab3:Button('Break In Story', function()
BKX()
end)
Tab3:Label('Status: Unsupported !')

local Tab4 = Library:CreateTab('4')

Tab4:Label('Executor')
Tab4:Button('Executor', function()
AXIS()
game.CoreGui.ShadowLib:Destroy()
end)
Tab4:Label('Other Script')
Tab4:Button('Shiftlock', function()
loadstring(game:HttpGet('https://pastebin.com/raw/WQ9NPeDS'))();
end)
Tab4:Button('Naked', function()
game.Players.LocalPlayer.Character.Shirt:Remove()
game.Players.LocalPlayer.Character.Pants:Remove()
end)
Tab4:Button('Full Bright', function()
local Light = game:GetService("Lighting")

function dofullbright()
Light.Ambient = Color3.new(1, 1, 1)
Light.ColorShift_Bottom = Color3.new(1, 1, 1)
Light.ColorShift_Top = Color3.new(1, 1, 1)
end

dofullbright()

Light.LightingChanged:Connect(dofullbright)
end)
Tab4:Label('Status: Working')

local Tab5 = Library:CreateTab('5')

Tab5:Label('Setting')
Tab5:Button('Unlock FPS', function()
print('[ ! ] Removed some models to trash.')
end)
Tab5:Button('No AntiCheat', function()
print('Vulkan has disabled but [Volt] models server has run.')
end)
Tab5:Button('Kill Loader', function()
game.CoreGui.ShadowLib:Destroy()
end)

end

function BKX()

-- Gui to Lua
-- Version: 3.2

-- Instances:

