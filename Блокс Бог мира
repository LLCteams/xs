function createfolders(path)
 local pathtbl = string.split(path, "/")
 for i, v in pairs(pathtbl) do
  if i == 1 then
   if not isfolder(v) then
    makefolder(v)
   end
  else
   local newpath = pathtbl[1]
   for i2=2, i-1 do
    newpath = newpath.. "/" ..pathtbl[i2]
   end
   newpath = newpath.. "/" ..v
   if not isfolder(newpath) then
    makefolder(newpath)
   end
  end
 end
end


if not isfolder("ECCS_Searcher_V2.6") then
createfolders("ECCS_Searcher_V2.6")
end

if not isfile("ECCS_Searcher_V2.6/Executor.ECCS") then
writefile("ECCS_Searcher_V2.6/Executor.ECCS", ExecutorTextBox.Text)
end

if not isfile("ECCS_Searcher_V2.6/AutoExecute.ECCS") then
writefile("ECCS_Searcher_V2.6/AutoExecute.ECCS", " ")
end

