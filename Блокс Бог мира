AntiFlingFunction:Disconnect()
end
end
end)

game:GetService("LogService").MessageOut:Connect(
function(Message)
repeat
wait(.1)
until Message
logTable[#logTable + 1] = Message
ConsoleFrame.Text = (table.concat(logTable, "\n"))
for i, v in pairs(logTable) do
if i == 12 then
table.remove(logTable, 1)
end
end
end)

