function lololol()
 local MESSAGE_DURATION = 5

local player = game.Players.LocalPlayer

local function equipAllTools()
    for _, tool in pairs(player.Backpack:GetChildren()) do
        if tool:IsA("Tool") then
            tool.Parent = player.Character
        end
    end
end

local function useTool()
    for _, tool in pairs(player.Character:GetChildren()) do
        if tool:IsA("Tool") then
            tool:Activate()
        end
    end
end

local function displayIntroductoryMessage()
    local gui = Instance.new("ScreenGui")
    gui.Name = "IntroMessageGui"
    gui.ResetOnSpawn = false
    gui.Parent = game.Players.LocalPlayer.PlayerGui
    
    local frame = Instance.new("Frame")
    frame.Name = "IntroMessageFrame"
    frame.Size = UDim2.new(0.3, 0, 0.1, 0)
    frame.Position = UDim2.new(0.5, -frame.Size.X.Offset / 2, 0.5, -frame.Size.Y.Offset / 2)
    frame.BackgroundColor3 = Color3.fromRGB(150, 110, 255)
    frame.BackgroundTransparency = 0.5
    frame.BorderSizePixel = 0
    frame.Parent = gui
    
    local textLabel = Instance.new("TextLabel")
    textLabel.Name = "IntroMessageText"
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.Position = UDim2.new(0, 0, 0, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.Text = "FE Multi-Gear User Script | Click E to equip all tools"
    textLabel.TextColor3 = Color3.new(1, 1, 1)
    textLabel.TextScaled = true
    textLabel.Parent = frame
    
    local countdownFrame = Instance.new("Frame")
    countdownFrame.Name = "CountdownFrame"
    countdownFrame.Size = UDim2.new(0.3, 0, 0.05, 0)
    countdownFrame.Position = UDim2.new(0.5, -countdownFrame.Size.X.Offset / 2, 0.6, 0)
    countdownFrame.BackgroundColor3 = Color3.fromRGB(150, 110, 255)  -- Light purple color
    countdownFrame.BackgroundTransparency = 0.5
    countdownFrame.BorderSizePixel = 0
    countdownFrame.Parent = gui
    
    local countdownLabel = Instance.new("TextLabel")
    countdownLabel.Name = "CountdownLabel"
    countdownLabel.Size = UDim2.new(1, 0, 1, 0)
    countdownLabel.Position = UDim2.new(0, 0, 0, 0)
    countdownLabel.BackgroundTransparency = 1
    countdownLabel.Text = tostring(MESSAGE_DURATION)
    countdownLabel.TextColor3 = Color3.new(1, 1, 1)
    countdownLabel.TextScaled = true
    countdownLabel.Parent = countdownFrame
    
    local function updateCountdown()
        for i = MESSAGE_DURATION, 1, -1 do
            countdownLabel.Text = tostring(i)
            wait(1)
        end
        gui:Destroy()
    end
    
    updateCountdown()
end

displayIntroductoryMessage()

local function onKeyPress(input)
    if input.KeyCode == Enum.KeyCode.E then
        equipAllTools()
    end
end

game:GetService("UserInputService").InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        useTool()
    end
end)

game:GetService("UserInputService").InputBegan:Connect(onKeyPress)
end


