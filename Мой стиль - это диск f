function ofall() 

-- dont execute it from here or you'll regret..

local TweenService = game:GetService("TweenService")

-- Create the ScreenGui
local gui = Instance.new("ScreenGui")
gui.Name = "Gazergui"
gui.Parent = game.CoreGui

-- Create the main frame and set its properties
local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0.3, 0, 0.3, 0)
mainFrame.Position = UDim2.new(0.30, 0, 0.30, 0)
mainFrame.BackgroundColor3 = Color3.new(0, 0, 0)-- Dark grey color
mainFrame.BorderSizePixel = 1
mainFrame.Active = true
mainFrame.BackgroundTransparency = 1 -- Start with fully transparent
mainFrame.Draggable = false -- Disable dragging the frame
mainFrame.Parent = gui

-- Create the main TextLabel and set its properties
local loadingLabel = Instance.new("TextLabel")
loadingLabel.Size = UDim2.new(1, 0, 0.8, 0)
loadingLabel.Position = UDim2.new(0, 0, 0, 0) -- Adjusted position for better centering
loadingLabel.BackgroundTransparency = 1 -- No background for the text label
loadingLabel.Text = "Universe X {•} Ultra"
loadingLabel.TextSize = 30
loadingLabel.TextColor3 = Color3.new(255, 0, 0) -- Cyan text color
loadingLabel.Font = Enum.Font.SourceSansBold -- Bold font
loadingLabel.TextTransparency = 1 -- Start with text fully transparent
loadingLabel.Parent = mainFrame

-- Create the small TextLabel and set its properties
local loadingSubLabel = Instance.new("TextLabel")
loadingSubLabel.Size = UDim2.new(1, 0, 0.2, 0)
loadingSubLabel.Position = UDim2.new(0, 0, 0.5, 0) -- Positioned below the main label
loadingSubLabel.BackgroundTransparency = 1 -- No background for the text label
loadingSubLabel.Text = "Please wait, load the files.."
loadingSubLabel.TextSize = 20
loadingSubLabel.TextColor3 = Color3.new(0, 255, 0) -- White text color
loadingSubLabel.Font = Enum.Font.SourceSansBold -- Regular font
loadingSubLabel.TextTransparency = 1 -- Start with text fully transparent
loadingSubLabel.Parent = mainFrame

-- Create the YT TextLabel and set its properties
local ytLabel = Instance.new("TextLabel")
ytLabel.Size = UDim2.new(0, 200, 0, 50)
ytLabel.Position = UDim2.new(-0.1, 0, 0.70, 0) -- Bottom left corner with some padding
ytLabel.BackgroundTransparency = 1 -- No background for the text label
ytLabel.Text = "["..game.Players.LocalPlayer.Name.."]"
ytLabel.TextSize = 14
ytLabel.TextColor3 = Color3.new(1, 1, 1) -- White text color
ytLabel.Font = Enum.Font.SourceSansBold -- Regular font
ytLabel.TextTransparency = 1 -- Start with text fully transparent
ytLabel.Parent = mainFrame

-- Create the Discord TextLabel and set its properties
local discordLabel = Instance.new("TextLabel")
discordLabel.Size = UDim2.new(0, 200, 0, 50)
discordLabel.Position = UDim2.new(-0.07, 0, 0.78, 0) -- Below the YT label with some padding
discordLabel.BackgroundTransparency = 1 -- No background for the text label
discordLabel.Text = "v0.2.6 | Ultra"
discordLabel.TextSize = 14
discordLabel.TextColor3 = Color3.new(10, 150, 200) -- White text color
discordLabel.Font = Enum.Font.SourceSansBold -- Regular font
discordLabel.TextTransparency = 1 -- Start with text fully transparent
discordLabel.Parent = mainFrame

-- Tween settings
local fadeInTime = 6
local stayTime = 18
-- 9999999999999999999999999
local fadeOutTime = 2

-- Fade in the frame and text
local fadeIn = TweenService:Create(mainFrame, TweenInfo.new(fadeInTime), {BackgroundTransparency = 0})
local fadeInLabel = TweenService:Create(loadingLabel, TweenInfo.new(fadeInTime), {TextTransparency = 0})
local fadeInSubLabel = TweenService:Create(loadingSubLabel, TweenInfo.new(fadeInTime), {TextTransparency = 0})
local fadeInYtLabel = TweenService:Create(ytLabel, TweenInfo.new(fadeInTime), {TextTransparency = 0})
local fadeInDiscordLabel = TweenService:Create(discordLabel, TweenInfo.new(fadeInTime), {TextTransparency = 0})

-- Fade out the frame and text
local fadeOut = TweenService:Create(mainFrame, TweenInfo.new(fadeOutTime), {BackgroundTransparency = 1})
local fadeOutLabel = TweenService:Create(loadingLabel, TweenInfo.new(fadeOutTime), {TextTransparency = 1})
local fadeOutSubLabel = TweenService:Create(loadingSubLabel, TweenInfo.new(fadeOutTime), {TextTransparency = 1})
local fadeOutYtLabel = TweenService:Create(ytLabel, TweenInfo.new(fadeOutTime), {TextTransparency = 1})
local fadeOutDiscordLabel = TweenService:Create(discordLabel, TweenInfo.new(fadeOutTime), {TextTransparency = 1})

-- Sequence of tweens
fadeIn:Play()
fadeInLabel:Play()
fadeInSubLabel:Play()
fadeInYtLabel:Play()
fadeInDiscordLabel:Play()

fadeIn.Completed:Connect(function()
    wait(stayTime)
    fadeOut:Play()
    fadeOutLabel:Play()
    fadeOutSubLabel:Play()
    fadeOutYtLabel:Play()
    fadeOutDiscordLabel:Play()
end)

fadeOut.Completed:Connect(function()
    gui:Destroy()
    local success, err = pcall(function()
------ loadstring(game:HttpGet("https://pastebin.com/raw/4bn47Jb0"))()
       semuaitem() 
    end)
    if not success then
        warn("Failed to load remote script: "..err)
    end
end)

-- Animate the "Loading gui..." text
local dots = {".", "..", "..."}
local index = 1

spawn(function()
    while true do
        loadingSubLabel.Text = "We setting everything" .. dots[index]
        index = index % #dots + 1
        wait(0.2) 
    end
end)

