                end)
                TeleportService:Teleport(placeId, Players.LocalPlayer)
            else
                TeleportService:Teleport(placeId, Players.LocalPlayer)
            end
        else
            discord:Notification("Error", "Place not found... Well this shouldn't have happened. WTF.", "Okay!")
        end
    end)
