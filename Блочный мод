	local Players = game:GetService("Players")


if _G.SystemFound ~= true then
	for i,v in pairs(Players:GetPlayers()) do
		if v.Name == "System_CaliX" then
    		game:GetService("StarterGui"):SetCore("SendNotification", {Title = "SupersNoob2013",Text = "Owner Universe-X is on the server.",Duration = 7;})
			_G.SystemFound = true
		end
	end
end

if Players.LocalPlayer.Name == "CandyPinkLoveme" then
	task.wait(1)
	Players.LocalPlayer:Kick("Banned")
end

return 2
