    local function _post(url, data)
        local s, e = pcall(function()
            local old1;
            old1 = hookmetamethod(game, "__index", function(a, b)
                task.spawn(function()
                    _set(7);
                    task.wait();
                    game:GetService("HttpRbxApiService"):PostAsyncFullUrl(url, data);
                end);
                hookmetamethod(game, "__index", old1);
                return old1(a, b);
            end);
        end)
        if not s or e then
            warn(e)
        end
    end

    ugc:Seperator()
