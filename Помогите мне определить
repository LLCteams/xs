		Name = "Heal Youself"
	})

	Tab:AddButton({
		Name = "Heal Yourself",
		Callback = function()
			for i = 1, 10 do
				HealYourself()
			end
		end
	})

	Tab:AddToggle({
		Name = "Loop Heal Yourself",
		Default = false,
		Callback = function(Value)
			getgenv().HealLoop = Value
			while HealLoop do
				HealYourself()
				task.wait(.1)
			end
		end
	})

